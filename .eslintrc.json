{
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": 1,
    "no-unused-vars": 1,
    "no-param-reassign": [2, { "props": false }],
    // "indent": ["warn", 2],
    "no-console": "warn"
  }
}
